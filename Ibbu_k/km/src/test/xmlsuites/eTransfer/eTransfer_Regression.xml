<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="eTransfer Regression Suite" >
    <parameter name="grid" value="local"/>
    <parameter name="udid" value="Kroger"/>
    <parameter name="mobilePlatformName" value="android"/>
    <parameter name="mobilePlatformVersion" value="8.1"/>
    <parameter name="androidDevice" value="emulator"/>
    <parameter name="appiumPort" value="4723"/>
    <parameter name="driverType" value="Appium"/>
    <parameter name="wait" value="30"/>
    <parameter name="androidApkPath" value="src\test\resources\eTransfer\apk\app-qa-0.30.3-qa.apk"/>
    <parameter name="androidApkPackage" value="com.kroger.sps.mobile.qa"/>
    <parameter name="androidApkActivity" value="com.kroger.sps.mobile.activity.TaskListActivity"/>

    <test name="eTransfer Regression Suite">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <packages>
            <package name="com.krogerqa.appium.product_locator_app.eTransfer.cases"/>
        </packages>
    </test>
</suite>
