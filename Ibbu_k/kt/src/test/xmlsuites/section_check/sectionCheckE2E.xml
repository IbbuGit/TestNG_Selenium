<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Section Check E2E Test Suite" >
    <parameter name="grid" value="local"/>
    <parameter name="udid" value="ZebraTC20"/>
    <parameter name="mobilePlatformName" value="Android"/>
    <parameter name="mobilePlatformVersion" value="8.1"/>
    <parameter name="androidDevice" value="emulator"/>
    <parameter name="appiumPort" value="4723"/>
    <parameter name="driverType" value="Appium"/>
    <parameter name="wait" value="30"/>
    <parameter name="androidApkPath" value="src\test\resources\section_check/apk/Dev.apk"/>
    <parameter name="androidApkPackage" value="com.kroger.sps.mobile.debug"/>
    <parameter name="androidApkActivity" value="com.kroger.sps.mobile.LoginActivity"/>
    <test name="Section Check - E2E Suite">
        <groups>
            <run>
                <include name="e2e"/>
            </run>
        </groups>
        <packages>
            <package name="com.krogerqa.appium.product_locator_app.section_check.cases"></package>
        </packages>
    </test>
</suite>
